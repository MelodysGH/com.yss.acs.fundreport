<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.yss.acs.fundreport.vo.ProductModeManage" table="t_mc_productModeManage" dynamic-update="false"
		dynamic-insert="true">
		<!-- BasalVO -->
		<id name="id" type="java.lang.String">
            <column name="FID" />
            <!-- 采用framework中的NUID生成器自动生成20为的数组id -->
            <generator class="com.yss.sofa.framework.id.hibernate.HibNUIDGenerator" />
		</id>

		<property name="fcode" type="string">
			<column name="FCODE" />
		</property>

		<property name="fname" type="string">
			<column name="FNAME" />
		</property>

		<property name="finitFlag" type="integer">
			<column name="FINT_FLAG" />
		</property>

		<property name="fjsType" type="string">
			<column name="FJS_TYPE" />
		</property>

		<property name="fpyDays" type="integer">
			<column name="FPY_DAYS" />
		</property>

		<property name="ftwiceVal" type="integer">
			<column name="FTWICE_VAL" />
		</property>

		<property name="fsetcode" type="integer">
			<column name="FSETCODE" />
		</property>
	</class>
</hibernate-mapping>
